<div class="container">
    <table class="table table-striped">
        <thead>
            <th>Nome</th>
            <th>Email</th>
        </thead>
        <tbody>
            {{#each model as |user|}}
                <tr>
                <td>
                    {{user.name}}
                </td>
                <td>
                    {{user.email}}
                </td>
                </tr>
            {{/each}}
        </tbody>
    </table>
</div>
